local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BaconBoss Hub | âš¡ Every Second Get +1 Speed", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


getgenv().tpPlace = "string"

function Teleport(teleportPlace)
    local player = game.Players.LocalPlayer
    player.Character.HumanoidRootPart.CFrame = teleportPlace
end

local TpTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp2Tab = Window:MakeTab({
	Name = "Egg",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp4Tab = Window:MakeTab({
	Name = "Others",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


_G.win = true 
function win()
     while _G.win == true do 
        Teleport(game:GetService("Workspace").Underwater.RaceEnter:GetChildren()[2].CFrame)
        wait(0)
        local args = {
            [1] = 2
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EnterRace"):FireServer(unpack(args))
     wait(0)
     Teleport(game:GetService("Workspace").Underwater.winparts:GetChildren()[4].award.CFrame)
     wait(0)
    end
end

_G.win2 = true 
function win2()
     while _G.win2 == true do 
        local args = {
            [1] = "Start",
            [2] = "Olympics"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("TournamentHandler"):FireServer(unpack(args))
     wait(0)
     Teleport(game:GetService("Workspace").WinRace.EndWall.CFrame)
     wait(0)   
    end
end

TpTab:AddToggle({
	Name = "Auto Farm Speed Best",
	Default = false,
	Callback = function(Value)
		_G.win = Value
        win()
	end    
})
 
TpTab:AddToggle({
	Name = "Auto Farm Win Best",
	Default = false,
	Callback = function(Value)
		_G.win2 = Value
        win2()
	end    
})

_G.rb = true
function rb()
     while _G.rb == true do
        local args = {
            [1] = "Rebirth"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("RebirthHandler"):FireServer(unpack(args))
     wait(0.5)
    end
end

TpTab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(Value)
		_G.rb = Value
        rb()
	end    
})

_G.segg = "egg1"
_G.egg = true
function egg()
     while _G.egg == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("PetHandler"):FireServer("OpenEgg", _G.segg)
        wait(0)
    end
end

Tp2Tab:AddToggle({
	Name = "Auto Hatch",
	Default = false,
	Callback = function(Value)
		_G.egg = Value
        egg()
	end    
})

Tp2Tab:AddDropdown({
	Name = "Select",
	Default = "egg1",
	Options = {"egg1", "egg2", "egg3", "egg4", "egg5", "egg6", "egg7", "egg8", "egg9"},
	Callback = function(Value)
	    _G.segg = Value  
	end    
})

Tp4Tab:AddButton({
    Name = "Anti AFK",
    Callback = function()
        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        end)
    end
})

Tp4Tab:AddButton({
    Name = "Delete Time Hold Button E",
    Callback = function()
        while true do
            for _, v in next, workspace:GetDescendants() do
                if v:IsA("ProximityPrompt") then
                    v.HoldDuration = 0
                end
            end
            wait(1) 
        end
    end
})

Tp4Tab:AddButton({
    Name = "Server Less People",
    Callback = function()
        local Http = game:GetService("HttpService")
        local TPS = game:GetService("TeleportService")
        local Api = "https://games.roblox.com/v1/games/"

        local _place = game.PlaceId
        local _servers = Api.._place.."/servers/Public?sortOrder=Asc&limit=100"

        local function ListServers(cursor)
            local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
            return Http:JSONDecode(Raw)
        end

        local Server, Next
        repeat
            local Servers = ListServers(Next)
            Server = Servers.data[1]
            Next = Servers.nextPageCursor
        until Server

        TPS:TeleportToPlaceInstance(_place, Server.id, game.Players.LocalPlayer)
    end
})

Tp4Tab:AddButton({
    Name = "Server Hop",
    Callback = function()
        local module = loadstring(game:HttpGet("https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua"))()
        module:Teleport(game.PlaceId)
    end
})

Tp4Tab:AddSlider({
	Name = "Change Speed Power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Tp4Tab:AddSlider({
	Name = "Change Jump Power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})
